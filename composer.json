{
    "name": "cdowdy/link2section",
    "description": "A Bolt Twig Filter to Link to sections or headers.",
    "type": "bolt-extension",
    "keywords": [
        "twig filter",
        "header link",
        "section link"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cory Dowdy",
            "email": "cory@corydowdy.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\cdowdy\\Link2Section\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\cdowdy\\Link2Section\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\cdowdy\\Link2Section\\Link2SectionExtension",
        "bolt-icon": "bolt-icon.png",
        "bolt-screenshots": [
            "screenshots/l2s-non-wrapping-screen.png",
            "screenshots/l2s-wrapping-screen.png",
            "screenshots/l2s-wrapping-with-class-screen.png"
        ]
    }
}
